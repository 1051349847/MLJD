/* pages/user/user.wxss */

page {
  background: #f3f3f3;
  height: 100%;
  overflow: visible;
}

.nones {
  display: none;
}

.blocks {
  display: block;
}

/* .tops{
  position: fixed;
  top:0rpx;
  width: 100%;
} */

.user {
  width: 100%;
  height: 240rpx;
  background: #fff;
  margin-bottom: 10rpx;
  display: flex;
  justify-content: space-between;
  margin-top: 10rpx
}

.user-name {
  display: flex;
}

.img_1 {
  width: 170rpx;
  height: 170rpx;
  overflow: hidden;
  display: inline-block;
  border-radius: 50%;
  margin-top:20rpx;
  margin-left: 15rpx;

  /* border: 8rpx solid rgb(240, 197, 146); */
  /* box-shadow: 3px 3px 10px rgba(0, 0, 0, 0.2); */
}

.user-name image {
  width: 160rpx;
  height: 160rpx;
  margin-left: 20rpx;
  margin-top: 40rpx;
  border-radius: 100%;
}

.name {
  font-size: 34rpx;
  color: rgb(51, 51, 51);
  line-height: 240rpx;
  margin-left: 20rpx;
  display: inline-block;
  /* height: 160rpx; */
}

.user-money {
  text-align: center;
  margin-right: 20rpx;
}

.money {
  color: rgb(244, 107, 24);
  font-size: 36rpx;
  margin-top: 116rpx;
  margin-bottom: 20rpx;
}

.money-cont {
  color: rgb(102, 102, 102);
  font-size: 28rpx;
  /* padding-bottom: 40rpx; */
}

.feedback {
  width: 100%;
  height: 84rpx;
  line-height: 84rpx;
  color: rgb(102, 102, 102);
  font-size: 34rpx;
  text-align: center;
}

.tops {
  position: fixed;
  width: 100%;
}

.swiper-box {
  height: 700rpx;
}

.swiper-box swiper-item {
  width: 100%;
  overflow: scroll;
  height: 100%;
}

scroll-view {
  height: 100%;
  /* margin-bottom: 80rpx; */
}

.swiper-tab {
  border-top: 1rpx solid #e5e5e5;
  width: 100%;
  /* border-bottom: 2rpx solid #777777; */
  text-align: center;
  height: 90rpx;
  background: #fff;
  display: flex;
  justify-content: space-between;
  line-height: 90rpx;
}

.swiper-tab-list {
  font-size: 32rpx;
  color: #999;
  margin-left: 20rpx;
}

.swiper-tab-list1 {
  font-size: 32rpx;
  color: #999;
}

.swiper-tab-list2 {
  font-size: 32rpx;
  color: #999;
  margin-right: 20rpx;
}

.on {
  color: #3687ff;
  border-bottom: 5rpx solid #3687ff;
}

.page {
  height: 100%;
  /* position: relative; */
}

.navbar__title {
  color: #999;
}

.navbar {
  display: flex;
  position: fixed;
  z-index: 500;
  top: 0;
  width: 100%;
  background: #fff;
}

.navbar__item {
  position: relative;
  display: block;
  flex: 1;
  padding: 10rpx 0;
  text-align: center;
  font-size: 0;
  height: 48rpx;
  line-height: 48rpx;
  width: 90rpx;
  /* <!-- NavBar的总高度为：height + padding-top + padding-bottom = 68rpx --> */
}

.navbar__item_on  .navbar__title {
  color: #f18d00;
}

.navbar__slider {
  /* position: absolute; */
  display: block;
  width: 90rpx;
  /* color: red; */
  content: " ";
  left: 0;
  bottom: 0;
  height: 3px;
  background-color: #16b13a;
  transition: transform 0.3s;
}

.navbar__title {
  display: inline-block;
  font-size: 32rpx;
  /* color:#333333; */
  max-width: 8em;
  text-align: center;
}

.seach_li {
  width: 710rpx;
  height: 100%;
}

.seach_li li {
  display: block;
  width: 710rpx;
  height: 345rpx;
  margin-left: 20rpx;
  background: #fff;
  margin-top: 20rpx;
  border-radius: 10rpx;
  position: relative;
}

.seach-img1 {
  width: 680rpx;
  height: 89rpx;
  border-bottom: 1rpx solid #e5e5e5;
  margin-left: 30rpx;
  text-align: right;
  line-height: 90rpx;
  font-size: 32rpx;
}

.cont-name {
  line-height: 90rpx;
  height: 90rpx;
  float: left;
}

.seach-img1 image {
  width: 16rpx;
  height: 25rpx;
  margin-right: 30rpx;
  vertical-align: middle;
}

.deta {
  width: 680rpx;
  height: 250rpx;
  text-align: left;
  margin-left: 30rpx;
  font-size: 28rpx;
  color: #999;
}

.site, .ms, .sect {
  width: 100%;
  display: block;
  margin-top: 30rpx;
}

.yy-but {
  width: 158rpx;
  height: 60rpx;
  border-radius: 10rpx;
  background: #f18d00;
  float: right;
  margin-right: 30rpx;
  margin-bottom: 7rpx;
  font-size: 30rpx;
  line-height: 60rpx;
  padding: 0;
  color: #fff;
  position: absolute;
  bottom: 30rpx;
  right: 0rpx;
}

button::after {
  border: 0;
}

.audit-a {
  display: flex;
  background: #fff;
  margin-top: 10rpx;
  height: 220rpx;
}

.audit-a image {
  width: 280rpx;
  height: 180rpx;
  margin-left: 20rpx;
  margin-top: 20rpx;
}

.audit-a-cont {
  margin-left: 20rpx;
}

.con-a {
  font-size: 28rpx;
  color: rgb(51, 51, 51);
  margin-top: 17rpx;
  margin-right: 20rpx;
  line-height: 40rpx;
  width: 410rpx;
  display: -webkit-box;
-webkit-box-orient: vertical;
-webkit-line-clamp: 3;
overflow: hidden;
}

.con-b {
  font-size: 26rpx;
  width: 160rpx;
  height: 48rpx;
  border-radius: 24rpx;
  background: #3687ff;
  line-height: 48rpx;
  text-align: center;
  color: #fff;
  float: right;
  margin-right: 20rpx;
  margin-top: 20rpx;
}

.logn {
  width: 400rpx;
  height: 100rpx;
  margin: 0 auto;
  margin-top: 500rpx;
}

.logn button {
  outline: none;
  border: none;
  background: #f18d00;
  line-height: 100rpx;
  font-size: 32rpx;
  text-align: center;
  color: #fff;
  border-radius: 20rpx;
}
